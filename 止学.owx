        <Class IRI="#06-情卷"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-01_情滥无行，欲多失矩"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-02_其色如一，神鬼莫测"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-03_上无度失威，下无忍數"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-04_上下知离，其位自安"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-05_君臣殊密，其臣反殃"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-06_小人之荣，情不可攀也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-07_情存疏也，近不过己，智者无痴焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-08_情难追也，逝者不返，明者无悔焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-09_多情者多艰，寡情者少艰"/>
    </Declaration>
    <Declaration>
        <Class IRI="#06-10_情之不敛，运无幸耳"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-蹇卷"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-01_人困乃正，命顺乃奇"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-02_以正化奇，止为枢也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-03_事变非智勿晓，事本非止勿存"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-04_天灾示警，逆之必亡。人祸告诫，省之固益"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-05_躁生百端，困出妄念，非止莫阻害止蔓焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-06_视己勿重者重，视人为轻者轻"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-07_患以心生，以蹇为乐，蹇不为蹇矣"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-08_穷不言富，贱不趋贵"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-09_忍辱为大，不怒为尊"/>
    </Declaration>
    <Declaration>
        <Class IRI="#07-10_蹇非敌也，敌乃乱焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-释怨卷"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-01_世之不公，人怨难止"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-02_穷富为仇，弥祸不消"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-03_君子不念旧恶，旧恶害德也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-04_小人存隙必报，必报自毁也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-05_和而弗争，谋之首也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-06_名不正而谤兴，正名者必自屈也焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-07_惑不解而恨重，释惑者固自罪焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-08_私念不生，仇怨不结焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-09_宽不足以悦人，严堪补也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#08-10_敬无助于劝善，诤堪教矣"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-心卷"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-01_欲无止也，其心堪制"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-02_惑无尽也，其行乃解"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-03_不求于人，其尊弗伤"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-04_无嗜之病，其身靡失"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-05_自弃者人莫救也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-06_苦乐无形，成于心焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-07_荣辱存异，贤者同焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-08_事之未济，志之非达，心无怨而忧患弗加矣"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-09_仁者好礼，不欺其心也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#09-10_智者示愚，不显其心哉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-修身卷"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-01_服人者德也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-02_德之不修，其才必曲，其人非善矣"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-03_纳言不失，不辍亡废"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-04_小处容疵，大节堪毁"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-05_敬人敬心，德之厚也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-06_诚非致虚，君子不行诡道"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-07_祸由己生，小人难于胜己"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-08_谤言无惧，强者不纵，堪验其德焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-09_不察其德，非识人也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#10-10_识而勿用，非大德也"/>
    </Declaration>
