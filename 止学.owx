        <Class IRI="#01-01_智极则愚也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#01-02_圣人不患智寡，患德之有失焉"/>
    </Declaration>
    <Declaration>
        <Class IRI="#01-03_才高非智，智者弗显也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#01-04_位尊实危，智者不就也"/>
