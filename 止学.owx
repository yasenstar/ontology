        <Class IRI="#04-03_誉存其伪，谄者以誉欺人"/>
    </Declaration>
    <Declaration>
        <Class IRI="#04-03_识不逾人，莫言断也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#04-04_势不及人者，休言讳也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#04-04_名不由己，明者言不自赞"/>
    </Declaration>
    <Declaration>
        <Class IRI="#04-05_力不胜人者，勿言强也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#04-05_贪巧之功，天不佑也"/>
    </Declaration>
    <Declaration>
        <Class IRI="#04-06_王者不辩，辩则少威焉"/>
