    <!-- http://www.semanticweb.org/yasen/ontologies/2023/6/止学#10-04_小处容疵，大节堪毁 -->

    <owl:Class rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#10-04_小处容疵，大节堪毁">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#10-修身卷"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/yasen/ontologies/2023/6/止学#10-05_敬人敬心，德之厚也 -->

    <owl:Class rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#10-05_敬人敬心，德之厚也">
