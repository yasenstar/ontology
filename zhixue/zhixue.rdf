    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-智卷"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-用势卷"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-利卷"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-辩卷"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#05-誉卷"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-情卷"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-蹇卷"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#08-释怨卷"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#09-心卷"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#10-修身卷"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-01_智极则愚也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-02_圣人不患智寡，患德之有失焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-03_才高非智，智者弗显也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-04_位尊实危，智者不就也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-05_大智知止，小智惟谋，智有穷而道无尽哉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-06_谋人者成于智，亦丧于智也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-07_谋身者恃其智，亦舍其智也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-08_智有所缺，深存其敌，慎之少祸焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-09_智不及而谋大者毁，智无歇而谋远者逆"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#01-10_智者言智，愚者言愚，以愚饰智，以智止智，智也"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-01_势无常也，仁者勿恃"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-02_势伏凶也，智者不矜"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-03_势莫加君子"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-04_德休与小人"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-05_君子势不于力也，力尽而势亡焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-06_小人势不惠人也，趋之必祸焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-07_众成其势，一人堪毁"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-08_强者凌弱，人怨乃弃"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-09_势极无让者疑，位尊弗恭者忌"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-10_势或失之，名或谤之，少怨者再得也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#02-11_势固灭之，人固死之，无骄者惠嗣焉"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-01_惑人者无逾利也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-02_利无求弗获，德无施不积"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-03_众逐利而富寡，贤让功而名高"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-04_利大伤身，利小惠人，择之宜慎也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-05_天贵于时，人贵于明，动之有戒也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-06_众见其利者，非利也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-07_众见其害者，或利也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-08_君子重义轻利，小人嗜利远信，利御小人而莫御君子矣"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-09_利无尽处，命有尽时，不怠可焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#03-10_利无独据，运有兴衰，存畏警焉"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-01_好誉者多辱也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-02_誉满主惊，名高众之所忌焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-03_誉存其伪，谄者以誉欺人"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-04_名不由己，明者言不自赞"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-05_贪巧之功，天不佑也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-06_赏誉勿轻，轻者誉贱，贱则无功也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-07_受誉知辞，辞则德显，显则释疑也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-08_上下无争，誉之不废焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-09_人无誉堪存，誉非正当灭"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-10_求誉不得，或为福也"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-01_物朴乃存，器工招损"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-02_言拙意隐，辞尽锋出"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-03_识不逾人，莫言断也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-04_势不及人者，休言讳也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-05_力不胜人者，勿言强也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-06_王者不辩，辩则少威焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-07_智者讷言，讷则惑敌焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-08_勇者无语，语则怯行焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-09_忠臣不表其功，窃功者必奸也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#04-10_君子堪隐人恶，谤贤者固小人矣"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-01_情滥无行，欲多失矩"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-02_其色如一，神鬼莫测"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-03_上无度失威，下无忍數"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-04_上下知离，其位自安"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-05_君臣殊密，其臣反殃"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-06_小人之荣，情不可攀也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-07_情存疏也，近不过己，智者无痴焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-08_情难追也，逝者不返，明者无悔焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-09_多情者多艰，寡情者少艰"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#06-10_情之不敛，运无幸耳"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-01_人困乃正，命顺乃奇"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-02_以正化奇，止为枢也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-03_事变非智勿晓，事本非止勿存"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-04_天灾示警，逆之必亡。人祸告诫，省之固益"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-05_躁生百端，困出妄念，非止莫阻害止蔓焉"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-06_视己勿重者重，视人为轻者轻"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-07_患以心生，以蹇为乐，蹇不为蹇矣"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-08_穷不言富，贱不趋贵"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-09_忍辱为大，不怒为尊"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#07-10_蹇非敌也，敌乃乱焉"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#08-01_世之不公，人怨难止"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#08-02_穷富为仇，弥祸不消"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#08-03_君子不念旧恶，旧恶害德也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#08-04_小人存隙必报，必报自毁也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#08-05_和而弗争，谋之首也"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/yasen/ontologies/2023/6/止学#08-06_名不正而谤兴，正名者必自屈也焉"/>
