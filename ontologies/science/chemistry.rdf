    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#Ca(OH)2 -->

    <owl:Class rdf:about="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#Ca(OH)2">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#化合物"/>
        <chemistry:化学反应式>氯气与Ca(OH)2反应生成CaCl2和Ca(ClO)2，漂白粉的有效成分是Ca(ClO)2：
2Ca(OH)2 + 2Cl2 == CaCl2 + Ca(ClO)2 + 2H2O</chemistry:化学反应式>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#Fe(OH)2_氢氧化亚铁 -->

    <owl:Class rdf:about="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#Fe(OH)2_氢氧化亚铁">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#化合物"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#铁的化合物"/>
        <chemistry:化学反应式>氢氧化亚铁与氢氧化铁的转换：
4Fe(OH)2 + O2 + 2H2O == 4Fe(OH)3</chemistry:化学反应式>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#Fe(OH)3_氢氧化铁 -->

    <owl:Class rdf:about="http://www.semanticweb.org/v0cn037/ontologies/2023/9/chemistry#Fe(OH)3_氢氧化铁">
